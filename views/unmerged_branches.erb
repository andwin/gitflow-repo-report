<div class="report-section">
  <h2 id="<%= id %>"><%= branches.count %> <%= title %></h2>
  <% if branches.count > 0 %>
  <div class="clear"></div>
  <table class="table-striped branch-list branch-list-unmerged">
    <tr>
      <th>
        Repo
      </th>
      <th>
        Branch
      </th>
      <th>
        Days since last commit
      </th>
      <th>
        Unmerged commits
      </th>
    </tr>

    <% branches.each do |branch| %>
    <tr>
      <td>
        <%= branch.repo_name %>
      </td>
      <td>
        <%= branch.name %>
      </td>
      <td>
        <%= branch.days_since_last_commit %>
      </td>
      <td>
        <%= branch.number_of_unmerged_commits %>
        <%= erb :dialog_commits, :layout => false, locals: {branch: branch} %>
      </td>
    </tr>
    <% end %>
  </table>
  <% end %>
</div>
<div class="clear"></div>