<h1>Report generated <%=@report.date %></h1>

<h2><%= @report.repos.count %> repos in report</h2>
<ul>
	<% @report.repos.each do |repo| %>
		<li><%= repo %></li>
	<% end %>
</ul>

<h2><%= @report.old_feature_branches.count %> Old feature branches</h2>
<ul>
	<% @report.old_feature_branches.each do |branch| %>
		<li><%= branch %></li>
	<% end %>
</ul>

<h2><%= @report.unmerged_release_branches.count %> Unmerged release branches</h2>
<ul>
	<% @report.unmerged_release_branches.each do |branch| %>
		<li><%= branch %></li>
	<% end %>
</ul>

<h2><%= @report.repos_with_unmerged_master_branch.count %> Master branch with commits missing from develop</h2>
<ul>
	<% @report.repos_with_unmerged_master_branch.each do |branch| %>
		<li><%= branch %></li>
	<% end %>
</ul>